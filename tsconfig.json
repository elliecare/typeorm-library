{
  "compilerOptions": {
    "lib": ["es5", "es6", "es2016"],
    "target": "es6",
    "resolveJsonModule": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "typeRoots": ["./src/@types", "./node_modules/@types"],
    "strict": false,
    "rootDir": ".",
    "baseUrl": ".",
    "esModuleInterop": true,
  }
}